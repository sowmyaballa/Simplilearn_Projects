package com.simplilearn.arrays;

public class MissingNumber07 {

	public static void main(String[] args) {
		int arr[] = {1,2,3,5,6};
		
		for(int i=0;i<arr.length;i++) {
			for(int j=i+1;j<arr.length;j++) {
				if(arr[i+2] != arr[j+i]) {
					System.out.println("Missing number is :"+ arr[i+1]);
				}
			}
		}

	}

}
