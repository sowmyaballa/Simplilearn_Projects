package com.simplilearn.arrays;

public class WithoutZero12 {

	public static void main(String[] args) {
		int a[] = {0,5,6,9,1};
		
		boolean flag = true;
		
		for(int i: a) {
			if( i==0 || i== -1) {
				flag = false;
				break;
			}
			
		}
		if(flag) {
			System.out.println("No 0 or -1");
		}
		else {
			System.out.println("0 or -1 present in array");
		}

	}

}
