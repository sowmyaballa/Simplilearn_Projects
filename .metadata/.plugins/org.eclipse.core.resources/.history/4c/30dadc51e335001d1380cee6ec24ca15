package com.simplilearn.arraylist;

import java.util.ArrayList;
import java.util.List;

public class CloneArrayToAnother_07 {

//	public static void main(String[] args) {
//		List<String> list = new ArrayList<>();
//		list.add("Sowmya");
//		list.add("Blessy");
//		list.add("Neeraja");
//		list.add("Sindhu");
